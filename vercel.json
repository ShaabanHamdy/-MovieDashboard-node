{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
          {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1"
    }
    ]
  }